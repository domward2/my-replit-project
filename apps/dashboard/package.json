{
  "name": "@dex/dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm --filter @dex/ui build && pnpm --filter @dex/types build && pnpm --filter @dex/evm-connect build && pnpm --filter @dex/solana-connect build",
    "dev": "next dev -p ${PORT:-3000}",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "typecheck": "tsc -p .",
    "test": "vitest run",
    "e2e": "playwright test"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@tanstack/react-query": "^5.60.5",
    "zod": "^3.24.2",
    "@dex/types": "workspace:*",
    "@dex/evm-connect": "workspace:*",
    "@dex/solana-connect": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.10",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "@playwright/test": "^1.45.2"
  }
}