{
  "name": "dex-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.8",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "turbo run build || pnpm -r --workspace-concurrency=1 run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "e2e": "turbo run e2e",
    "format": "prettier -w .",
    "start": "pnpm --filter @dex/dashboard start"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "turbo": "^2.0.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "tsx": "^4.7.2",
    "@playwright/test": "^1.45.2"
  }
}
